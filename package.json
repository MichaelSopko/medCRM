  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack.config.js"
        }
      }
    },
    "env": {
      "mocha": true
    },
    "globals": {
      "window": true,
      "document": true,
      "__DEV__": true,
      "__CLIENT__": true,
      "__SERVER__": true,
      "__SSR__": true
