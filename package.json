    "graphql-subscriptions": "0.3.0",
    "graphql-tag": "1.2.1",
    "graphql-tools": "0.11.0",
    "graphql-type-json": "0.1.4",
    "graphql-typings": "0.0.1-beta-2",
    "isomorphic-fetch": "2.2.1",
    "jquery-file-upload-middleware": "0.1.8",
    "jsonwebtoken": "7.2.1",
    "knex": "0.12.1",
    "lodash": "^4.17.4",
    "minilog": "3.0.1",
    "moment": "2.17.1",
    "multer": "1.3.0",
    "mysql": "2.13.0",
    "nodemailer": "4.0.0",
    "npm-run-all": "^4.1.2",
    "postcss": "5.2.15",
